{
  "name": "claroid",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
  	"frontend/*",
  	"backend/*",
  	"libs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
    },
  "devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"husky": "^8.0.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
		"npm-run-all": "^4.1.5"
  },
  "engines": {
  	"node": ">=19.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.27.1"
}
