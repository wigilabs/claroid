{
  "name": "claroid",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "frontend/*",
    "backend/*",
    "libs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.27.1",
  "engines": {
    "node": ">=19.0.0"
  }
}
